{
  "name": "puppeteer-confluence-setup",
  "version": "1.0.0",
  "description": "A puppeteer based script to automate the initial setup of Confluence for testing purposes ",
  "main": "src/main/confluence-setup.ts",
  "scripts": {
    "start": "npm run clean; testcafe firefox src/main/confluence-setup.ts",
    "clean": "rimraf screenshots/*.png, screenshots/thumbnails",
    "lint": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aruizca/puppeteer-confluence-setup.git"
  },
  "keywords": [
    "confluence",
    "testcafe",
    "setup",
    "automation"
  ],
  "author": "@aruizca",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aruizca/puppeteer-confluence-setup/issues"
  },
  "homepage": "https://github.com/aruizca/puppeteer-confluence-setup#readme",
  "dependencies": {
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.22.0",
    "eslint-plugin-functional": "4.4.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "testcafe": "2.0.1"
  }
}
